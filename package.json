{
  "name": "os.api.document",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "proto-gen": "yarn proto-loader-gen-types --grpcLib=@grpc/grpc-js --keepCase --outDir=src/configs/rpc/protos/ src/configs/rpc/protos/*.proto",
    "migrate:up": "sequelize db:migrate",
    "seed": "ts-node src/scripts/importMasterData.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.13",
    "@grpc/proto-loader": "^0.7.10",
    "@types/crypto-js": "^4.1.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.11.10",
    "dotenv": "^10.0.0",
    "expo-server-sdk": "^3.10.0",
    "express": "^4.21.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "firebase-admin": "^12.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.2.5",
    "node-cron": "^3.0.0",
    "node-mailjet": "^6.0.5",
    "nodemailer": "^6.6.5",
    "passport": "^0.7.0",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "4.0.1",
    "sequelize": "6.6.5",
    "sequelize-cli": "6.2.0",
    "sharp": "^0.33.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.10.1",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-formidable": "^1.2.3",
    "@types/express-session": "^1.17.4",
    "@types/formidable": "1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.171",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/passport": "^1.0.16",
    "@types/passport-anonymous": "^1.0.5",
    "@types/passport-jwt": "3.0.13",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^2.0.9",
    "typescript": "^5.7.3"
  },
  "_moduleAliases": {
    "@configs": "dist/configs",
    "@database": "dist/database",
    "@entities": "dist/entities",
    "@initializers": "dist/initializers",
    "@interfaces": "dist/interfaces",
    "@models": "dist/models",
    "@services": "dist/services",
    "@controllers": "dist/controllers",
    "@middlewares": "dist/middlewares",
    "@workers": "dist/workers",
    "~": "dist",
    "@libs": "dist/libs",
    "@decorators": "dist/decorators",
    "@jobs": "dist/jobs"
  },
  "xo": {
    "extends": ".eslintrc.json"
  }
}
